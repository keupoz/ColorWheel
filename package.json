{
  "name": "colorwheel",
  "version": "1.0.3",
  "description": "Simple HSV color wheel for web pages",
  "main": "build/ColorWheel.js",
  "jsnext:main": "build/ColorWheel.module.js",
  "module": "build/ColorWheel.module.js",
  "files": [
    "LICENSE",
    "package.json",
    "build"
  ],
  "scripts": {
    "build": "rollup -c && uglifyjs build/ColorWheel.js -o build/ColorWheel.min.js -cm",
    "dev": "rollup -cw"
  },
  "keywords": [
    "hsv",
    "hsl",
    "color",
    "wheel",
    "picker",
    "html"
  ],
  "license": "MIT",
  "devDependencies": {
    "rollup": "^0.62.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-json": "^3.0.0",
    "uglify-js": "^3.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keupoz/ColorWheel.git"
  },
  "author": "keupoz",
  "bugs": {
    "url": "https://github.com/keupoz/ColorWheel/issues"
  },
  "homepage": "https://github.com/keupoz/ColorWheel#readme"
}
